# API Configuration
REACT_APP_API_URL=http://localhost:4000/api
REACT_APP_WS_URL=ws://localhost:4000

# Authentication
REACT_APP_AUTH_DOMAIN=your-auth0-domain
REACT_APP_AUTH_CLIENT_ID=your-auth0-client-id

# Supabase
REACT_APP_SUPABASE_URL=https://yourproject.supabase.co
REACT_APP_SUPABASE_KEY=public-anon-key

# Feature Flags
REACT_APP_ESTIMATOR_ENABLED=true
REACT_APP_AI_COPILOT_ENABLED=true
REACT_APP_MAINTENANCE_MODE=false

# Optional Analytics
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_SENTRY=false

# Build Info
REACT_APP_VERSION=$npm_package_version
REACT_APP_BUILD_TIME=$(date -u +"%Y-%m-%dT%H:%M:%SZ")
